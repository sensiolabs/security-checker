{
    "directories": ["vendor","SensioLabs"],
    "main": "security-checker",
    "output": "security-checker.phar",
    "files": [
        "LICENSE",
        "README.md",
        "SensioLabs/Security/Resources/security.sensiolabs.org.crt",
        "SensioLabs/Security/Resources/security.sensiolabs.org.crt.sha"
